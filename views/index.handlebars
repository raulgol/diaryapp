<!--LOGIN-->
<div data-role="page" id="login" data-add-back-btn="false">
	<!--Login Header-->
	<div data-role="header" data-theme="b">
		<h1>TR&#916;CK</h1>
		<a href="#info" data-rel="dialog" data-icon="info" data-iconpos="notext" class="ui-btn-right"></a>
	</div>
	<!--/Login Header-->

	<!--Login Content-->
	<div data-role="content" data-theme="c">	
		<!--Logo--><center><img src="http://imgur.com/fmf0r2Z" />

		<!--Login Form-->
		<form method="PUT" action="api/auth" data-ajax="false">
    		<input type="text" name="login_username" id="login_username" placeholder="username"/>
    		<input type="password" name="login_password" id="login_password" placeholder="password"/>
		</form>		
		<a href="#home" data-role="button">Sign In</a>
		<a href="#forgotpasswordprompt" data-rel="dialog" data-transition="fade">forgot your password?</a>
	</div>

	<!--/Login Content -->

	<!--Login Footer-->
	<div data-role="footer" data-id="fool" data-position="fixed" data-theme="b" >
		<div data-role="navbar" data-theme="a" data-iconpos="left">
			<ul>
				<li><a href="#register" data-transition="slideup" data-icon="carat-u">Create an Account</a></li>
			</ul>
		</div>
	</div><!--/Login Footer -->

</div><!-- /page -->

<!--REGISTER/SIGN UP-->
<div data-role="page" id="register" data-add-back-btn="false">
	<!--Header-->
	<div data-role="header" data-theme="b" data-add-back-btn="true">
		<h1>Create an Account</h1>
				<a href="#info" data-icon="info" data-iconpos="notext" class="ui-btn-right"></a>
	</div>
	<!--/Header-->

	<!--Content-->
	<div data-role="content" data-theme="c">
		<h3>Registration Details</h3>
		<!--Registration form-->
		<input type="text" required name="first_name" placeholder="First Name">
		<input type="text" required name="last_name" placeholder="Last Name">
		<input type="text" required name="username" placeholder="Username">
		<input type="email" required name="email" placeholder="Email">
		<input type="password" required name="password" placeholder="Password">
		<input type="password" required name="confirm_password" placeholder="Confirm Password">
		<a href="#login" data-role="button" data-transition="slideup" data-direction="reverse">Submit</a>
	</div>
	<!--/Content-->

	<!--Footer-->
	<div data-role="footer" data-id="fool" data-position="fixed" data-theme="b">
		<div data-role="navbar" data-theme="a" data-iconpos="left">
			<ul>
				<li><a href="#login" class="ui-btn-active ui-state-persist" data-transition="slideup" data-direction="reverse" data-icon="carat-d">Cancel</a></li>
			</ul>
		</div> 
	</div>
	<!--/Footer -->
</div>
<!--/REGISTRATION/SIGN UP-->

<!--INFO PAGE-->
<div data-role="page" id="info" data-add-back-btn="true">
	<div data-role="header" data-theme="b">
		<h3>Info</h3>
	</div>
	<div data-role="content" data-theme="c">
